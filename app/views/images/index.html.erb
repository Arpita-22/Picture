
<form class="new_image" id="new_image" action="/images" multipart=true method="post">
    <label for="title">Title</label>
    <input type="text" name="image[title]" id="image_title">
    <br>
    <label for="tag">Tag</label>
    <input type="text" name="image[tag]" id="image_tag">
    <br>
    <input type="file" name="fileToUpload" id="fileToUpload">
    <br>
    <input type="submit" name="commit" value="upload image" data-disable-with="upload image">
</form>

